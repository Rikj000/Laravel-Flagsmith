{
    "name": "rikj000/laravel-flagsmith",
    "description": "A Flagsmith client for Laravel",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrew Nagy",
            "email": "anagy@clearlyip.com"
        },
        {
            "name": "Rik Helsen",
            "email": "rik_helsen@hotmail.com",
            "homepage": "https://github.com/rikj000",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=8.0",
        "flagsmith/flagsmith-php-client": ">=2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rikj000\\LaravelFlagsmith\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Rikj000\\LaravelFlagsmith\\": "src"
        }
    },
    "require-dev": {
        "guzzlehttp/psr7": "^2.1",
        "guzzlehttp/guzzle": "^7.4"
    }
}